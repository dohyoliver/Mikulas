generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model Jatekok{
  id Int @id @default(autoincrement())
   megnevezes String
  anyag String
  suly Int
  gyerekek Gyerekek[] @relation("JatekGyerek")
}

model Gyerekek{
  id Int @id @default(autoincrement())
  nev String
  cim String
  jovolte Boolean
  
  jatekok Jatekok[] @relation("JatekGyerek")
}

